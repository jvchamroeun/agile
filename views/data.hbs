                      <th style= "padding:20px" name="datetime">Time/Date</th>
                      <th style= "padding:20px" name="stock">Stock</th>
                      <th style= "padding:20px" name="amount">Amount</th>
                  </tr>{{reverse result}}
                        {{#each result}}
                            <tr>
                                <td style= "padding:20px">{{this.date}}</td>
                                <td style= "padding:20px">{{this.stock}}</td>
                                <td style= "padding:20px">{{this.quantity}}</td>
                          {{/each}}</tr>
